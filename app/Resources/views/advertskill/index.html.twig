{% extends 'base.html.twig' %}

{% block body %}
    <h1>AdvertSkill list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Level</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for advertSkill in advertSkills %}
            <tr>
                <td><a href="{{ path('advertskill_show', { 'id': advertSkill.id }) }}">{{ advertSkill.id }}</a></td>
                <td>{{ advertSkill.level }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('advertskill_show', { 'id': advertSkill.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('advertskill_edit', { 'id': advertSkill.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('advertskill_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
